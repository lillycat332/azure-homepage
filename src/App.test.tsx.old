import React from 'react';
import { render, screen } from '@testing-library/react';
import App from './App';

test('renders text', () => {
	render(
		<React.StrictMode>
			<App />
		</React.StrictMode>
	);
	const linkElement = screen.getByText(/Projects/i);
	expect(linkElement).toBeInTheDocument();
});